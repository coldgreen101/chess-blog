<div class="hamburger">
  <span class="line"></span>
  <span class="line"></span>
  <span class="line"></span>
</div>

<script>
  const hamburger = document.querySelector('.hamburger');
  hamburger.addEventListener('click', () => {
    const lines = document.querySelectorAll('.line');
    lines.forEach((line) => {
      line.classList.toggle('active');
    });
  });
</script>
